{
  "name": "vite-plugin-htmls",
  "version": "0.1.1",
  "description": "Vite Plugin that simplifies creation of HTML files to serve your bundles",
  "main": "dist/index.mjs",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "roll-plugin",
    "html",
    "template"
  ],
  "scripts": {
    "start": "tsup src/index.ts --format esm --dts --watch",
    "build": "rm -rf lib && tsup src/index.ts --format esm --dts",
    "test": "jest",
    "coverage": "codecov"
  },
  "author": "maoxiaoke@outlook.com",
  "license": "MIT",
  "devDependencies": {
    "@iceworks/spec": "^1.4.1",
    "eslint": "^7.32.0",
    "jest": "^27.1.1",
    "rollup": "^2.56.3",
    "ts-jest": "^27.0.5",
    "tsup": "^5.3.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.6"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "ts-jest"
  },
  "dependencies": {}
}
